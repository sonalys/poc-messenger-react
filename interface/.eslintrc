{
  "parser": "@typescript-eslint/parser",
  "env": {
    "jasmine": true,
    "node": true,
    "browser": true,
    "jest/globals": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": [
          "node_modules",
          "src/"
        ]
      }
    }
  },
  "plugins": [
    "jest",
  ]
}